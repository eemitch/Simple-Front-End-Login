msgid "Basic Front-End Login"
msgstr "Login di base per il Front-End"

#: includes/ee-functions.php:315
msgid "Version"
msgstr "Versione"

#: includes/ee-functions.php:315
msgid "Plugin by"
msgstr "Plugin di"

